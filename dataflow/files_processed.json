{
	"name": "files_processed",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Json1",
						"type": "DatasetReference"
					},
					"name": "browsertrend"
				},
				{
					"dataset": {
						"referenceName": "Json2",
						"type": "DatasetReference"
					},
					"name": "mbaapps"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Parquet1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				},
				{
					"dataset": {
						"referenceName": "Parquet1",
						"type": "DatasetReference"
					},
					"name": "mbaappsprocessed"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine') ~> browsertrend",
				"source(output(",
				"          body as (data as (capacity as string, color as string, {capacity GB} as string, price as string, generation as string, {CPU model} as string, {Hard disk size} as string, year as string, {Case Size} as string, {Strap Colour} as string, Color as string, Description as string, Capacity as string, {Screen size} as string), id as string, name as string),",
				"          headers as (Vary as string, {Report-To} as string, {Transfer-Encoding} as string, Server as string, Connection as string, {CF-Cache-Status} as string, {Content-Type} as string, Date as string, NEL as string, {CF-RAY} as string, {alt-svc} as string)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine') ~> mbaapps",
				"browsertrend sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1",
				"mbaapps sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> mbaappsprocessed"
			]
		}
	}
}