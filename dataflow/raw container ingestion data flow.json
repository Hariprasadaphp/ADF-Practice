{
	"name": "raw container ingestion data flow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "free_rest_api",
						"type": "DatasetReference"
					},
					"name": "browsertrendsingestion"
				},
				{
					"dataset": {
						"referenceName": "RestResource4",
						"type": "DatasetReference"
					},
					"name": "mbaappsingestion"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Json1",
						"type": "DatasetReference"
					},
					"name": "browsertrendsraw",
					"rejectedDataLinkedService": {
						"referenceName": "storage acc linked service",
						"type": "LinkedServiceReference"
					}
				},
				{
					"dataset": {
						"referenceName": "Json2",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     httpMethod: 'GET',",
				"     timeout: 30,",
				"     requestInterval: 0,",
				"     paginationRules: ['supportRFC5988' -> 'true'],",
				"     responseFormat: ['type' -> 'json', 'documentForm' -> 'documentPerLine']) ~> browsertrendsingestion",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     httpMethod: 'GET',",
				"     timeout: 30,",
				"     requestInterval: 0,",
				"     paginationRules: ['supportRFC5988' -> 'true'],",
				"     responseFormat: ['type' -> 'json', 'documentForm' -> 'documentPerLine']) ~> mbaappsingestion",
				"browsertrendsingestion sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> browsertrendsraw",
				"mbaappsingestion sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          body as (data as (capacity as string, color as string, {capacity GB} as string, price as string, generation as string, {CPU model} as string, {Hard disk size} as string, year as string, {Case Size} as string, {Strap Colour} as string, Color as string, Description as string, Capacity as string, {Screen size} as string), id as string, name as string),",
				"          headers as (Vary as string, {Report-To} as string, {Transfer-Encoding} as string, Server as string, Connection as string, {CF-Cache-Status} as string, {Content-Type} as string, Date as string, NEL as string, {CF-RAY} as string, {alt-svc} as string)",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}