{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Adfdemoacg"
		},
		"storage acc linked service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'storage acc linked service'"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://reqres.in/api/users?page=2"
		},
		"RestService2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://api.apis.guru/v2/list.json"
		},
		"browser_trend_connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/awaheeddev/datafiles/main/browser_trends.csv"
		},
		"free_rest_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.restful-api.dev/objects"
		},
		"mba apps linked service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/awaheeddev/datafiles/main/mba_apps.csv"
		},
		"storage acc linked service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdemosgacc.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Adfdemoacg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"storage acc linked service_accountKey": {
						"value": "[parameters('storage acc linked service_accountKey')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"RestService2_properties_typeProperties_url": {
						"value": "[parameters('RestService2_properties_typeProperties_url')]"
					},
					"browser_trend_connection_properties_typeProperties_url": {
						"value": "[parameters('browser_trend_connection_properties_typeProperties_url')]"
					},
					"free_rest_api_properties_typeProperties_url": {
						"value": "[parameters('free_rest_api_properties_typeProperties_url')]"
					},
					"mba apps linked service_properties_typeProperties_url": {
						"value": "[parameters('mba apps linked service_properties_typeProperties_url')]"
					},
					"storage acc linked service_properties_typeProperties_url": {
						"value": "[parameters('storage acc linked service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Adfdemoacg_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Adfdemoacg_ArmTemplate_0')]"
			]
		}
	]
}